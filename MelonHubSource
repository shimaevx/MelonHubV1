local HWIDTable = loadstring(game:HttpGet("https://pastebin.com/raw/0tp8QssH"))()
local Whitelist = getgenv().HWIDTable.WhitelistedIDs)
local Blacklist = getgenv().HWIDTable.BlacklistedIDs)
local HWID = game:GetService("RbxAnalyticsService"):GetClientId()
local Player = game.Players.LocalPlayer

-- Made by $ melon#5256

--//-- Whitelist check
for Index, Whitelisted in pairs(Whitelist) do
    if Whitelisted ~= HWID then
        setclipboard(HWID)
----//
        else

---//-- Blacklist check
for 2Index, Blacklisted in pairs(Blacklist) do
if Blacklisted == HWID then
Player:Kick("Your device has been blacklisted from MelonHub")
else
print("Hardware is already whitelisted")
    end
 end
end